{% extends 'base.html.twig' %}
 
{% block title %} Sessions ! {% endblock %}
 
{% block body %}
<div class="content">

    <div id="ficheStagiaire">
        <img id="icons" src="{{ asset('img/education.png') }}" alt="sessions"> &nbsp;&nbsp;&nbsp;&nbsp;<h1>Liste des Sessions</h1>
    </div>

    {% for session in sessions %}
        <ul class="list-group list-group-flush">   
            <li class="list-group-item"> <a href="{{ path('show_session',{'id':session.id })}}"> {{ session.nom }} </a>  </li>
        </ul>             
    {% endfor %}
    
</div>
{% endblock %}
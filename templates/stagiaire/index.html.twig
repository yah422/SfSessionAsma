{% extends 'base.html.twig' %}

{% block title %} Stagiaires ! {% endblock %}

{% block body %}

<div class="content">

    <div id="ficheStagiaire">
        <img id="icons" src="{{ asset('img/graduation-hat.png') }}" alt="Graduation Hat"> &nbsp;&nbsp;&nbsp;&nbsp;<h1>Liste des stagiaires</h1>
    </div>
    
    {% for stagiaire in stagiaires %}
        <ul class="list-group list-group-flush">
            <li class="list-group-item"> <a href="{{ path('show_stagiaire',{'id':stagiaire.id })}}"> {{ stagiaire.nom}} {{stagiaire.prenom }} </a></li>
        </ul>
    {% endfor %}
 
</div>
{% endblock %}
